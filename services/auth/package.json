{
  "name": "authorization",
  "version": "0.0.0",
  "license": "MIT",
  "main": "./dist/app.js",
  "private": true,
  "type": "module",
  "exports": "./dist/app.js",
  "scripts": {
    "start2": "if [[ $NODE_ENV == 'production' ]]; then node -r esm ./app.js; else nodemon -r esm ./app.js; fi",
    "watch": "nodemon --watch './**/*.{ts,graphql}' --exec 'node --experimental-specifier-resolution=node --loader ts-node/esm' src/app.ts",
    "start": "tsc-watch --onSuccess \"npm run watch\"",
    "test": "ENV_FILE=./.env.test jest --coverage",
    "test:watch": "ENV_FILE=./.env.test jest --watch"
  },
  "dependencies": {
    "apollo-server-core": "^3.5.0",
    "apollo-server-express": "^3.5.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.1",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^10.0.0",
    "esm": "^3.2.25",
    "express": "~4.17.1",
    "express-graphql": "^0.12.0",
    "graphql": "^15.4.0",
    "graphql-passport": "^0.6.4",
    "graphql-tag": "^2.12.6",
    "http-errors": "~1.6.3",
    "joi-to-typescript": "^2.3.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "node-fetch": "^3.1.0",
    "nodemailer": "^6.7.2",
    "passport": "^0.5.0",
    "passport-jwt": "^4.0.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.0",
    "@types/lodash": "^4.14.178",
    "@types/node": "^16.11.12",
    "@types/nodemailer": "^6.4.4",
    "@types/passport-jwt": "^3.0.6",
    "@types/uuid": "^8.3.3",
    "jest": "^27.4.7",
    "jest-fetch-mock": "^3.0.3",
    "nodemon": "^2.0.15",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.4.0",
    "tsc-watch": "^4.5.0",
    "typescript": "^4.5.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kodirov257/nodejs-authorization.git"
